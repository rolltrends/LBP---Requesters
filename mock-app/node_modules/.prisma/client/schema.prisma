generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Requester {
  id          Int    @id @default(autoincrement())
  name        String
  first_name  String
  last_name   String
  gender      String
  phone_num   String
  mobile      String
  email_id    String @unique
  employee_id String
  job_title   String
  description String
}
